{
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "rules": {
    "interface-name": false,
    "object-literal-sort-keys": false,
    "no-consecutive-blank-lines": [true, 2],
    "ordered-imports": false,
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "max-line-length": false,
    "jsx-no-multiline-js": false,
    "member-ordering": false,
    "jsx-no-lambda": false,
    "arrow-parens": false
  }
}

